    "type": "project",
    "autoload": {
      "psr-4": {
        "App\\": "app/"
      }
    },
    "require": {
      "vlucas/phpdotenv": "^5.5",
      "firebase/php-jwt": "^6.11",
        "catfan/medoo": "^2.2"
    },
    "require-dev": {
      "phpunit/phpunit": "^10.0",
        "friendsofphp/php-cs-fixer": "^3.75",
        "phpstan/phpstan": "2.1.11"
    },
    "config": {
    "process-timeout": 3000
    },
    "scripts": {
      "phpstan": "vendor/bin/phpstan analyse -l 6 app"
    }
}
